{
  "log": {
    "loglevel": "warning",
    "error": "/var/log/xray/error.log", //若使用V2Ray，此处目录名称xray改成v2ray。
    "access": "/var/log/xray/access.log" //若使用V2Ray，此处目录名称xray改成v2ray。
  },
  "inbounds": [
    {
      "listen": "/dev/shm/trojanws.sock", //Trojan+WebSocket监听进程。若接收来自Caddy的转发，此监听进程修改为@trojanws.sock。
      "protocol": "trojan",
      "settings": {
        "clients": [
          {
            "password":"diy2007", //修改为自己密码
            "email": "2007@gmail.com"
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "path": "/9ALdGZ9k" //修改为自己的path
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "protocol": [
          "bittorrent"
        ],
        "outboundTag": "blocked"
      }
    ]
  },
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "blocked",
      "protocol": "blackhole",
      "settings": {}
    }
  ]
}
